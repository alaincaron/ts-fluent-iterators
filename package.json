{
  "name": "ts-fluent-iterators",
  "version": "5.0.0",
  "description": "lazy fluent iterators",
  "main": "./dist/index.js",
  "engines": {
    "node": "*"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "npm run build && mocha -r ts-node/register test/**/*.test.ts",
    "pristine": "rimraf ./dist ./node_modules",
    "build": "rimraf ./dist && tsc",
    "repl": "npm run build && env NODE_PATH=./dist ts-node -i",
    "htmldoc": "./scripts/htmldoc",
    "eslint": "eslint",
    "coverage": "nyc npm run test",
    "format": "prettier --write .",
    "precommit": "lint-staged",
    "prepare": "husky install .husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alaincaron/ts-fluent-iterators.git"
  },
  "keywords": [
    "lazy",
    "iterator",
    "typescript",
    "fluent",
    "async",
    "promise"
  ],
  "author": "Alain Caron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alaincaron/ts-fluent-iterators/issues"
  },
  "homepage": "https://github.com/alaincaron/ts-fluent-iterators#readme",
  "lint-staged": {
    "*.ts": "prettier --write",
    "*.json": "prettier --write",
    "*.js": "prettier --write"
  },
  "devDependencies": {
    "@types/chai": "^4.3.10",
    "@types/mocha": "^10.0.4",
    "chai": "^4.3.10",
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.1.0",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "types": "./dist/index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "files": [
    "dist"
  ]
}
